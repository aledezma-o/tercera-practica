{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": { // Configuracion para definir como se lanzara la aplicacion
    "Development": { //Perfil exclusivo para desarrollo
      "commandName": "Project", // del tipo proyecto que crea consola
      "launchBrowser": false, // lanzara el buscador por defecto
      "launchUrl": "", // el path donde se abrira en base a la aplicacion
      "applicationUrl": "http://localhost:5001", // en que direccion se levanta
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "QA": {
      "commandName": "Project",
      "launchBrowser": false,
      "launchUrl": "",
      "applicationUrl": "http://localhost:5002",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "QA"
      }
    },
    "Production": {
      "commandName": "Project",
      "launchBrowser": false,
      "launchUrl": "",
      "applicationUrl": "http://localhost:5003",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      }
    }
  }
}
